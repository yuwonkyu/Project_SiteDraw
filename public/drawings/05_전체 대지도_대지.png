<svg width="1600" height="1000" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1600 1000">
  <defs>
    <style>
      .grid { stroke: #e0e0e0; stroke-width: 0.5; }
      .building { stroke: #333; stroke-width: 2; fill: #ffd9b3; }
      .road { stroke: #999; stroke-width: 1; fill: #d3d3d3; }
      .parking { stroke: #ccc; stroke-width: 1; fill: #f0f0f0; }
      .green { stroke: #2d5016; stroke-width: 1; fill: #90ee90; }
      .water { stroke: #4da6ff; stroke-width: 1; fill: #e6f2ff; }
      .label { font-family: Arial; font-size: 14px; font-weight: bold; }
      .title { font-family: Arial; font-size: 20px; font-weight: bold; }
      .small-label { font-family: Arial; font-size: 11px; }
    </style>
  </defs>
  
  <!-- Background (Land) -->
  <rect width="1600" height="1000" fill="#f5f5f0" />
  
  <!-- Grid -->
  <g class="grid">
    <line x1="0" y1="0" x2="0" y2="1000" />
    <line x1="100" y1="0" x2="100" y2="1000" />
    <line x1="200" y1="0" x2="200" y2="1000" />
    <line x1="300" y1="0" x2="300" y2="1000" />
    <line x1="400" y1="0" x2="400" y2="1000" />
    <line x1="500" y1="0" x2="500" y2="1000" />
    <line x1="600" y1="0" x2="600" y2="1000" />
    <line x1="700" y1="0" x2="700" y2="1000" />
    <line x1="800" y1="0" x2="800" y2="1000" />
    <line x1="900" y1="0" x2="900" y2="1000" />
    <line x1="1000" y1="0" x2="1000" y2="1000" />
    <line x1="1100" y1="0" x2="1100" y2="1000" />
    <line x1="1200" y1="0" x2="1200" y2="1000" />
    <line x1="0" y1="100" x2="1600" y2="100" />
    <line x1="0" y1="200" x2="1600" y2="200" />
    <line x1="0" y1="300" x2="1600" y2="300" />
    <line x1="0" y1="400" x2="1600" y2="400" />
    <line x1="0" y1="500" x2="1600" y2="500" />
    <line x1="0" y1="600" x2="1600" y2="600" />
    <line x1="0" y1="700" x2="1600" y2="700" />
    <line x1="0" y1="800" x2="1600" y2="800" />
    <line x1="0" y1="900" x2="1600" y2="900" />
  </g>
  
  <!-- Main Road -->
  <g class="road">
    <rect x="0" y="350" width="1600" height="100" fill="#999" />
    <line x1="0" y1="400" x2="1600" y2="400" stroke="white" stroke-width="2" stroke-dasharray="20,10" />
  </g>
  
  <!-- Building 101 -->
  <rect class="building" x="100" y="80" width="180" height="240" />
  <text class="label" x="160" y="210">101동</text>
  
  <!-- Building 102 -->
  <rect class="building" x="320" y="80" width="180" height="240" />
  <text class="label" x="380" y="210">102동</text>
  
  <!-- Building 201 -->
  <rect class="building" x="540" y="80" width="180" height="240" />
  <text class="label" x="600" y="210">201동</text>
  
  <!-- Building 202 -->
  <rect class="building" x="760" y="80" width="180" height="240" />
  <text class="label" x="820" y="210">202동</text>
  
  <!-- Building 301 -->
  <rect class="building" x="980" y="80" width="180" height="240" />
  <text class="label" x="1040" y="210">301동</text>
  
  <!-- Building 302 -->
  <rect class="building" x="1200" y="80" width="180" height="240" />
  <text class="label" x="1260" y="210">302동</text>
  
  <!-- Building 401 -->
  <rect class="building" x="100" y="520" width="180" height="240" />
  <text class="label" x="160" y="650">401동</text>
  
  <!-- Building 402 -->
  <rect class="building" x="320" y="520" width="180" height="240" />
  <text class="label" x="380" y="650">402동</text>
  
  <!-- Building 403 -->
  <rect class="building" x="540" y="520" width="180" height="240" />
  <text class="label" x="600" y="650">403동</text>
  
  <!-- Building 404 -->
  <rect class="building" x="760" y="520" width="180" height="240" />
  <text class="label" x="820" y="650">404동</text>
  
  <!-- Community Building (커뮤니티 빌딩) -->
  <rect class="building" x="1000" y="520" width="180" height="120" fill="#e6ccb3" />
  <text class="label" x="1050" y="590">커뮤니티</text>
  
  <!-- Parking Lot A -->
  <rect class="parking" x="100" y="800" width="350" height="150" />
  <text class="small-label" x="260" y="880">주차장 A</text>
  
  <!-- Parking Lot B -->
  <rect class="parking" x="500" y="800" width="350" height="150" />
  <text class="small-label" x="660" y="880">주차장 B</text>
  
  <!-- Parking Lot C -->
  <rect class="parking" x="900" y="800" width="350" height="150" />
  <text class="small-label" x="1060" y="880">주차장 C</text>
  
  <!-- Green Space 1 -->
  <ellipse class="green" cx="1400" cy="150" rx="100" ry="80" />
  <text class="small-label" x="1380" y="160">조경</text>
  
  <!-- Green Space 2 -->
  <ellipse class="green" cx="1350" cy="650" rx="120" ry="100" />
  <text class="small-label" x="1325" y="660">정원</text>
  
  <!-- Water Feature -->
  <circle class="water" cx="1450" cy="450" r="60" />
  <text class="small-label" x="1440" y="460">분수</text>
  
  <!-- Pedestrian Path -->
  <path d="M 200 330 Q 400 290 600 330 Q 800 370 1000 330 Q 1200 290 1400 330" 
        stroke="#d4a574" stroke-width="15" fill="none" />
  
  <!-- Gates -->
  <rect x="50" y="380" width="30" height="40" fill="#999" />
  <text class="small-label" x="55" y="405">정문</text>
  
  <!-- Scale & North Arrow -->
  <g transform="translate(1450, 50)">
    <text class="label" x="0" y="0" style="font-size: 12px;">N</text>
    <line x1="0" y1="5" x2="0" y2="30" stroke="#333" stroke-width="2" marker-end="url(#arrowhead)" />
  </g>
  
  <!-- Title -->
  <text class="title" x="800" y="950" text-anchor="middle">전체 대지도 (Site Plan)</text>
</svg>