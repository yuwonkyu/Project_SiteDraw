<svg width="1600" height="1000" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1600 1000">
  <defs>
    <style>
      .wall { stroke: #333; stroke-width: 3; fill: #fff; }
      .room { stroke: #999; stroke-width: 1; fill: #fffacd; }
      .window { stroke: #0066cc; stroke-width: 2; fill: #cce5ff; }
      .door { stroke: #cc6600; stroke-width: 2; fill: none; }
      .grid { stroke: #ddd; stroke-width: 0.5; }
      .label { font-family: Arial; font-size: 16px; font-weight: bold; }
      .title { font-family: Arial; font-size: 20px; font-weight: bold; }
    </style>
  </defs>
  
  <!-- Grid -->
  <g class="grid">
    <line x1="0" y1="0" x2="0" y2="1000" />
    <line x1="50" y1="0" x2="50" y2="1000" />
    <line x1="100" y1="0" x2="100" y2="1000" />
    <line x1="150" y1="0" x2="150" y2="1000" />
    <line x1="200" y1="0" x2="200" y2="1000" />
    <line x1="0" y1="0" x2="1600" y2="0" />
    <line x1="0" y1="50" x2="1600" y2="50" />
    <line x1="0" y1="100" x2="1600" y2="100" />
  </g>
  
  <!-- Main Building Outline -->
  <rect class="wall" x="100" y="80" width="1000" height="700" />
  
  <!-- Room 1 -->
  <rect class="room" x="120" y="100" width="150" height="180" />
  <rect class="window" x="130" y="95" width="40" height="8" />
  <text class="label" x="195" y="200">거실</text>
  
  <!-- Room 2 -->
  <rect class="room" x="320" y="100" width="140" height="180" />
  <rect class="window" x="330" y="95" width="40" height="8" />
  <text class="label" x="360" y="200">침실1</text>
  
  <!-- Room 3 -->
  <rect class="room" x="510" y="100" width="140" height="180" />
  <rect class="window" x="520" y="95" width="40" height="8" />
  <text class="label" x="550" y="200">침실2</text>
  
  <!-- Room 4 -->
  <rect class="room" x="700" y="100" width="130" height="180" />
  <rect class="window" x="710" y="95" width="40" height="8" />
  <text class="label" x="730" y="200">침실3</text>
  
  <!-- Room 5 -->
  <rect class="room" x="880" y="100" width="180" height="180" />
  <rect class="window" x="890" y="95" width="40" height="8" />
  <text class="label" x="960" y="200">주방</text>
  
  <!-- Room 6 -->
  <rect class="room" x="120" y="320" width="180" height="160" />
  <text class="label" x="180" y="410">화장실</text>
  
  <!-- Room 7 -->
  <rect class="room" x="350" y="320" width="240" height="160" />
  <rect class="window" x="360" y="315" width="40" height="8" />
  <text class="label" x="460" y="410">복도</text>
  
  <!-- Room 8 -->
  <rect class="room" x="650" y="320" width="240" height="160" />
  <rect class="window" x="660" y="315" width="40" height="8" />
  <text class="label" x="750" y="410">드레스룸</text>
  
  <!-- Room 9 -->
  <rect class="room" x="950" y="320" width="140" height="160" />
  <text class="label" x="1005" y="410">세탁실</text>
  
  <!-- Room 10 -->
  <rect class="room" x="120" y="540" width="1000" height="120" />
  <text class="label" x="650" y="605">발코니</text>
  
  <!-- Doors -->
  <line class="door" x1="260" y1="280" x2="310" y2="280" />
  <line class="door" x1="450" y1="280" x2="500" y2="280" />
  <line class="door" x1="640" y1="280" x2="690" y2="280" />
  
  <!-- North Elevation Line -->
  <line x1="100" y1="80" x2="1100" y2="80" stroke="#000" stroke-width="4" />
  
  <!-- Title -->
  <text class="title" x="800" y="950" text-anchor="middle">101동 지상1층 평면도 (건축)</text>
</svg>