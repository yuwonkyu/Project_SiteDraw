<svg width="1600" height="1000" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1600 1000">
  <defs>
    <style>
      .grid { stroke: #fff; stroke-width: 0.5; }
      .wall { stroke: #333; stroke-width: 3; fill: #f5f5f5; }
      .air-duct { stroke: #ff6b6b; stroke-width: 3; fill: none; stroke-dasharray: 5,5; }
      .water-pipe { stroke: #4dabf7; stroke-width: 3; fill: none; }
      .electric-cable { stroke: #ffd43b; stroke-width: 3; fill: none; }
      .outlet { stroke: #333; stroke-width: 2; fill: #ffd43b; }
      .ac-unit { stroke: #ff6b6b; stroke-width: 2; fill: #ffe0e0; }
      .label { font-family: Arial; font-size: 12px; fill: #333; }
      .title { font-family: Arial; font-size: 20px; font-weight: bold; }
    </style>
  </defs>
  
  <!-- Background -->
  <rect width="1600" height="1000" fill="#fafafa" />
  
  <!-- Grid -->
  <g class="grid">
    <line x1="0" y1="0" x2="0" y2="1000" />
    <line x1="100" y1="0" x2="100" y2="1000" />
    <line x1="200" y1="0" x2="200" y2="1000" />
    <line x1="300" y1="0" x2="300" y2="1000" />
    <line x1="400" y1="0" x2="400" y2="1000" />
    <line x1="500" y1="0" x2="500" y2="1000" />
    <line x1="600" y1="0" x2="600" y2="1000" />
    <line x1="700" y1="0" x2="700" y2="1000" />
    <line x1="800" y1="0" x2="800" y2="1000" />
    <line x1="900" y1="0" x2="900" y2="1000" />
    <line x1="1000" y1="0" x2="1000" y2="1000" />
    <line x1="0" y1="50" x2="1600" y2="50" />
    <line x1="0" y1="100" x2="1600" y2="100" />
    <line x1="0" y1="150" x2="1600" y2="150" />
    <line x1="0" y1="200" x2="1600" y2="200" />
    <line x1="0" y1="250" x2="1600" y2="250" />
  </g>
  
  <!-- Room Walls -->
  <rect class="wall" x="120" y="100" width="800" height="700" />
  
  <!-- Air Ducts -->
  <path class="air-duct" d="M 200 150 L 200 250 L 800 250 L 800 700" />
  <path class="air-duct" d="M 300 150 L 300 300 L 700 300 L 700 650" />
  <path class="air-duct" d="M 450 150 L 450 400 L 600 400 L 600 600" />
  
  <!-- Water (Hot & Cold) Pipes -->
  <path class="water-pipe" d="M 180 150 L 180 450" stroke-width="4" />
  <path class="water-pipe" d="M 200 150 L 200 350" stroke-width="4" />
  <path class="water-pipe" d="M 220 150 L 220 550 L 400 550" stroke-width="4" />
  <path class="water-pipe" d="M 240 150 L 240 600 L 850 600" stroke-width="4" />
  
  <!-- Drain Pipes -->
  <path class="water-pipe" d="M 270 150 L 270 300 L 900 300" stroke="#51cf66" stroke-width="3" stroke-dasharray="3,3" />
  <path class="water-pipe" d="M 300 150 L 300 450 L 900 450" stroke="#51cf66" stroke-width="3" stroke-dasharray="3,3" />
  
  <!-- Electric Cables (Main) -->
  <path class="electric-cable" d="M 150 200 L 150 750" />
  <path class="electric-cable" d="M 170 200 L 170 650" />
  <path class="electric-cable" d="M 190 200 L 190 550" stroke-width="2" />
  
  <!-- Electrical Panels -->
  <rect class="outlet" x="140" y="180" width="20" height="25" />
  <text class="label" x="155" y="200">NB</text>
  
  <!-- Light fixtures -->
  <circle cx="350" cy="200" r="8" stroke="#ffd43b" stroke-width="2" fill="none" />
  <circle cx="500" cy="200" r="8" stroke="#ffd43b" stroke-width="2" fill="none" />
  <circle cx="650" cy="200" r="8" stroke="#ffd43b" stroke-width="2" fill="none" />
  
  <!-- Electrical Outlets -->
  <rect class="outlet" x="250" y="670" width="12" height="12" />
  <rect class="outlet" x="400" y="670" width="12" height="12" />
  <rect class="outlet" x="550" y="670" width="12" height="12" />
  <rect class="outlet" x="700" y="670" width="12" height="12" />
  
  <!-- AC Units -->
  <rect class="ac-unit" x="350" y="750" width="60" height="40" />
  <text class="label" x="380" y="775">AC 1</text>
  
  <rect class="ac-unit" x="600" y="750" width="60" height="40" />
  <text class="label" x="630" y="775">AC 2</text>
  
  <!-- Legend -->
  <g transform="translate(1100, 150)">
    <text class="label" style="font-weight: bold; font-size: 14px;" x="0" y="0">범례 (Legend)</text>
    
    <line class="air-duct" x1="0" y1="20" x2="40" y2="20" />
    <text class="label" x="50" y="25">공조덕트</text>
    
    <line class="water-pipe" x1="0" y1="50" x2="40" y2="50" />
    <text class="label" x="50" y="55">수도관</text>
    
    <path class="water-pipe" d="M 0 80 L 40 80" stroke="#51cf66" stroke-width="3" stroke-dasharray="3,3" />
    <text class="label" x="50" y="85">드레인</text>
    
    <line class="electric-cable" x1="0" y1="110" x2="40" y2="110" />
    <text class="label" x="50" y="115">전기배선</text>
    
    <circle cx="20" cy="140" r="6" stroke="#ffd43b" stroke-width="2" fill="none" />
    <text class="label" x="50" y="145">조명</text>
    
    <rect class="outlet" x="5" y="160" width="10" height="10" />
    <text class="label" x="50" y="170">콘센트</text>
    
    <rect class="ac-unit" x="0" y="190" width="35" height="25" />
    <text class="label" x="50" y="205">에어콘</text>
  </g>
  
  <!-- Title -->
  <text class="title" x="800" y="940" text-anchor="middle">101동 지상1층 MEP 설비도</text>
</svg>