<svg width="1600" height="1000" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1600 1000">
  <defs>
    <style>
      .grid { stroke: #ddd; stroke-width: 0.5; }
      .room { stroke: #f5a500; stroke-width: 2; fill: #fff9e6; }
      .wall { stroke: #333; stroke-width: 4; fill: #f0f0f0; }
      .window { stroke: #5a9cdb; stroke-width: 3; fill: #e8f4f8; }
      .door { stroke: #a8633e; stroke-width: 3; fill: none; }
      .label { font-family: Arial; font-size: 14px; font-weight: bold; }
      .title { font-family: Arial; font-size: 20px; font-weight: bold; }
    </style>
  </defs>
  
  <!-- Grid Background -->
  <g class="grid">
    <line x1="0" y1="0" x2="0" y2="1000" />
    <line x1="100" y1="0" x2="100" y2="1000" />
    <line x1="200" y1="0" x2="200" y2="1000" />
    <line x1="300" y1="0" x2="300" y2="1000" />
    <line x1="400" y1="0" x2="400" y2="1000" />
    <line x1="500" y1="0" x2="500" y2="1000" />
    <line x1="600" y1="0" x2="600" y2="1000" />
    <line x1="700" y1="0" x2="700" y2="1000" />
    <line x1="800" y1="0" x2="800" y2="1000" />
    <line x1="900" y1="0" x2="900" y2="1000" />
    <line x1="1000" y1="0" x2="1000" y2="1000" />
    <line x1="0" y1="50" x2="1600" y2="50" />
    <line x1="0" y1="100" x2="1600" y2="100" />
    <line x1="0" y1="150" x2="1600" y2="150" />
    <line x1="0" y1="200" x2="1600" y2="200" />
    <line x1="0" y1="250" x2="1600" y2="250" />
  </g>
  
  <!-- Outer Walls -->
  <rect class="wall" x="120" y="100" width="780" height="720" />
  
  <!-- Main Living Room (거실) -->
  <rect class="room" x="130" y="110" width="250" height="280" />
  <text class="label" x="230" y="260">거실</text>
  
  <!-- Kitchen -->
  <rect class="room" x="390" y="110" width="150" height="140" />
  <text class="label" x="420" y="190">주방</text>
  
  <!-- Dining Area -->
  <rect class="room" x="390" y="260" width="150" height="130" />
  <text class="label" x="400" y="330">식당</text>
  
  <!-- Bedroom 1 (마스터 침실) -->
  <rect class="room" x="560" y="110" width="180" height="200" />
  <text class="label" x="590" y="220">침실 1</text>
  
  <!-- Bedroom 2 -->
  <rect class="room" x="560" y="320" width="170" height="140" />
  <text class="label" x="580" y="395">침실 2</text>
  
  <!-- Bedroom 3 -->
  <rect class="room" x="750" y="110" width="140" height="200" />
  <text class="label" x="770" y="220">침실 3</text>
  
  <!-- Balcony (발코니) -->
  <rect class="room" x="750" y="320" width="140" height="140" />
  <text class="label" x="770" y="395">발코니</text>
  
  <!-- Bathroom 1 (화장실) -->
  <rect class="room" x="130" y="400" width="120" height="140" />
  <text class="label" x="140" y="475">화장실</text>
  
  <!-- Bathroom 2 -->
  <rect class="room" x="260" y="400" width="110" height="140" />
  <text class="label" x="270" y="475">욕실</text>
  
  <!-- Hallway (복도) -->
  <rect class="room" x="380" y="410" width="170" height="130" />
  <text class="label" x="420" y="480">복도</text>
  
  <!-- Entrance (현관) -->
  <rect class="room" x="560" y="480" width="150" height="140" />
  <text class="label" x="580" y="560">현관</text>
  
  <!-- Utility Room (세탁실) -->
  <rect class="room" x="720" y="480" width="130" height="140" />
  <text class="label" x="735" y="560">세탁실</text>
  
  <!-- Windows -->
  <!-- Kitchen Window -->
  <rect class="window" x="545" y="105" width="40" height="8" />
  
  <!-- Bedroom 1 Windows -->
  <rect class="window" x="560" y="105" width="35" height="8" />
  <rect class="window" x="610" y="105" width="35" height="8" />
  
  <!-- Bedroom 3 Windows -->
  <rect class="window" x="750" y="105" width="35" height="8" />
  
  <!-- Living Room Window -->
  <rect class="window" x="130" y="105" width="70" height="8" />
  
  <!-- Balcony Doors -->
  <rect class="window" x="750" y="315" width="60" height="8" />
  <rect class="window" x="830" y="315" width="60" height="8" />
  
  <!-- Doors -->
  <!-- Kitchen Door -->
  <path class="door" d="M 390 245 Q 375 245 375 230" />
  
  <!-- Bedroom 1 Door -->
  <path class="door" d="M 560 200 Q 545 200 545 185" />
  
  <!-- Bathroom Door -->
  <path class="door" d="M 250 400 Q 265 400 265 415" />
  
  <!-- Entrance Door -->
  <path class="door" d="M 560 520 Q 545 520 545 535" />
  
  <!-- Title -->
  <text class="title" x="800" y="930">101동 지상2층 평면도 (건축)</text>
</svg>